<h1><%= t('new_link') %></h1>
        <%= form_with(model: @link, local: true, class: 'col-10 offset-1') do |form| %>
            <%= render partial: 'fields', locals: {'form': form}%>
            <div class="d-flex justify-content-center">
                <%= form.submit t('form_save'), :class => 'col-4 btn btn-success' %>
            </div>
        <% end %>
